
div( data-role="panel" data-title-caption="ROUT LATENCY" )
    div.w-100
        div.row.border-bottom.bd-system.pb-2
            div.cell-6
                div.text-bold.text-center
                    div.reduce-3 Sum RPC Discover
                    div.text-leader2.text-bold.value#latency_by_route_sum_rpc_discover 0
            div.cell-6.border-left.bd-system
                div.text-bold.text-center
                    div.reduce-3 Count RPC Discover
                    div.text-leader2.text-bold.value#latency_by_route_count_rpc_discover 0

        div.row.pb-2
            div.cell-6
                div.text-bold.text-center
                    div.reduce-3 Sum Version
                    div.text-leader2.text-bold.value#latency_by_route_sum_version 0
            div.cell-6.border-left.bd-system
                div.text-bold.text-center
                    div.reduce-3 Count Version
                    div.text-leader2.text-bold.value#latency_by_route_count_version 0



